" .gvimrc
" v 0.1
" rev. 2009-03-18

" Change colorscheme
" colorscheme slate

" GUI Option to remove the Toolbar (T)
set guioptions-=T
set guioptions-=L
set guioptions-=r
set guioptions-=m

" Sets the percent transparency
" set transparency=5

set guifont=Monaco:h12
" set transparency=10
colorscheme dracula
